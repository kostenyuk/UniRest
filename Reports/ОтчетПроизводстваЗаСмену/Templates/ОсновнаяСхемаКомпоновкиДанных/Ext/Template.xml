ВЫБРАТЬ
	ВТДанные.Номенклатура,
	ВТДанные.Часы,
	ВТДанные.Минуты,
	ВТДанные.Показатель,
	ВТДанные.КолВо,
	ВТДанные.Приоритет,
	ВТДанные.Ресторан
ИЗ
	ВТДанные КАК ВТДанные

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВТДанные.Номенклатура,
	NULL,
	NULL,
	"Проготовлено всего",
	0,
	2,
	ВТДанные.Ресторан
ИЗ
	ВТДанные КАК ВТДанные

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВТДанные.Номенклатура,
	NULL,
	NULL,
	"Продано",
	0,
	3,
	ВТДанные.Ресторан
ИЗ
	ВТДанные КАК ВТДанные

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВТДанные.Номенклатура,
	NULL,
	NULL,
	"Списано",
	0,
	4,
	ВТДанные.Ресторан
ИЗ
	ВТДанные КАК ВТДанные

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВТДанные.Номенклатура,
	NULL,
	NULL,
	"Возвращено всего",
	0,
	6,
	ВТДанные.Ресторан
ИЗ
	ВТДанные КАК ВТДанные

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВложенныйЗапрос.Номенклатура,
	NULL,
	NULL,
	"Остаток",
	СУММА(ВложенныйЗапрос.КолВо),
	7,
	ВложенныйЗапрос.Ресторан
ИЗ
	(ВЫБРАТЬ
		ВТДанные.Номенклатура КАК Номенклатура,
		ВТДанные.Ресторан КАК Ресторан,
		ВТДанные.КолВо КАК КолВо
	ИЗ
		ВТДанные КАК ВТДанные
	ГДЕ
		ВТДанные.Приоритет = 2
	
	ОБЪЕДИНИТЬ ВСЕ
	
	ВЫБРАТЬ
		ВТДанные.Номенклатура,
		ВТДанные.Ресторан,
		-ВТДанные.КолВо
	ИЗ
		ВТДанные КАК ВТДанные
	ГДЕ
		ВТДанные.Приоритет В (3, 4, 6)) КАК ВложенныйЗапрос

СГРУППИРОВАТЬ ПО
	ВложенныйЗапрос.Номенклатура,
	ВложенныйЗапрос.Ресторан

ОБЪЕДИНИТЬ ВСЕ

ВЫБРАТЬ
	ВТДанные.Номенклатура,
	NULL,
	NULL,
	"Остаток",
	0,
	7,
	ВТДанные.Ресторан
ИЗ
	ВТДанные КАК ВТДанные
