			</UseAlways>
			<Settings xsi:type="DynamicList">
				<ManualQuery>true</ManualQuery>
				<DynamicDataRead>true</DynamicDataRead>
				<QueryText>ВЫБРАТЬ РАЗРЕШЕННЫЕ
	Данные.Ссылка,
	Данные.ВерсияДанных,
	Данные.ПометкаУдаления,
	Данные.Пользовательский,
	НЕ Данные.Пользовательский КАК Предопределенный,
	Данные.Наименование,
	Данные.ВидимостьПоУмолчанию,
	Данные.Отчет,
	Данные.КлючВарианта,
	Данные.ТипОтчета,
	Данные.Автор,
	Данные.Описание,
	ВЫБОР
		КОГДА Данные.ТипОтчета = &amp;ТипВнутренний
			ТОГДА Данные.Отчет.Имя
		//КОГДА Данные.ТипОтчета = &amp;ТипДополнительный
		//	ТОГДА Данные.Отчет
		//ИНАЧЕ ПОДСТРОКА(ВЫРАЗИТЬ(Данные.Отчет КАК СТРОКА(150)), 14, 137)
	КОНЕЦ КАК ИмяОтчета,
	ВЫБОР
		КОГДА Данные.ПометкаУдаления = ИСТИНА
			ТОГДА 4
		КОГДА Данные.Пользовательский = ЛОЖЬ
			ТОГДА 5
		ИНАЧЕ 3
	КОНЕЦ КАК ИндексКартинки
ИЗ
	Справочник.ВариантыОтчетов КАК Данные
ГДЕ
	//(&amp;РазделИлиГруппа = &amp;КорневаяПодсистема
			Данные.Размещение.РазделИлиГруппа В ИЕРАРХИИ (&amp;РазделИлиГруппа)
	И (&amp;ПоказыватьВсеОтчеты = ИСТИНА
			ИЛИ Данные.Ссылка.Отчет В (&amp;ОтчетыПользователя))
	И (&amp;ОтборНаименование = ""
			ИЛИ Данные.Наименование ПОДОБНО "%" + &amp;ОтборНаименование + "%")
	И (&amp;ОтборАвтор = ЗНАЧЕНИЕ(Справочник.Пользователи.ПустаяСсылка)
			ИЛИ Данные.Автор = &amp;ОтборАвтор)
	И ВЫБОР
			КОГДА &amp;ОтборТипОтчета = НЕОПРЕДЕЛЕНО
				ТОГДА ИСТИНА
			КОГДА &amp;ОтборТипОтчета = 1
				ТОГДА Данные.ТипОтчета = &amp;ТипВнутренний
						//ИЛИ Данные.ТипОтчета = &amp;ТипДополнительный
			ИНАЧЕ Данные.ТипОтчета = &amp;ОтборТипОтчета
		КОНЕЦ</QueryText>
